    {
      "path": "dist/assets/js/main-*.js",
      "maxSize": "150 KB",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/js/core-*.js",
      "maxSize": "100 KB",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/js/ui-*.js",
