      "compression": "gzip"
    },
    {
      "path": "dist/assets/js/storage-*.js",
      "maxSize": "25 KB",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/js/export-*.js",
      "maxSize": "40 KB",
      "compression": "gzip"
